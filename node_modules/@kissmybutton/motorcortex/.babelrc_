{
  "env": {
    "test": {
      "presets": ["@babel/preset-env"],
      "plugins": [
        "@babel/plugin-proposal-class-properties", [
          "@babel/plugin-proposal-decorators",
          {
            "legacy": false,
            "decoratorsBeforeExport": true
          }
        ]
      ]
    },
    "production": {
      "ignore": [
        "**/__tests__"
      ],
      "presets": ["@babel/preset-env", ["minify", {
        builtIns: false,
        evaluate: false,
        mangle: false,
      }]],
      "plugins": [
        "@babel/plugin-proposal-class-properties", [
          "@babel/plugin-proposal-decorators",
          {
            "legacy": false,
            "decoratorsBeforeExport": true
          }
        ]
      ]
    },
    "development": {
      "ignore": [
        "**/__tests__"
      ],
      "presets": ["@babel/preset-env"],
      "plugins": [
        "@babel/plugin-proposal-class-properties", [
          "@babel/plugin-proposal-decorators",
          {
            "legacy": false,
            "decoratorsBeforeExport": true
          }
        ]
      ]
    }
  }
}
